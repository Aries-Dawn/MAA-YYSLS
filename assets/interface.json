{
    // 本文件主要定义了通用 UI 的行为和显示内容
    // 可参考文档和示例添加更多您需要的功能
    // 文档：https://github.com/MaaXYZ/MaaFramework/blob/main/docs/zh_cn/3.3-ProjectInterfaceV2%E5%8D%8F%E8%AE%AE.md
    // 示例：https://github.com/MaaXYZ/MaaFramework/blob/main/sample/interface.json
    "interface_version": 2,
    "name": "MaaYYSLS",
    "title": "MaaYYSLS",
    "description": "好像可以自动化？",
    "welcome": "首次使用！欢迎使用 MAA!",
    "icon": "logo.ico",
    // 填写项目地址和版本号可实现自动更新功能
    "github": "https://github.com/Aries-Dawn/MAA-YYSLS",
    "version": "1.0.1",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb",
            "display_short_side": 720 // 默认缩放分辨率的短边长度
        },
        {
            "name": "桌面端",
            "type": "Win32",
            "win32": {
                //"class_regex": ".*",
                "window_regex": "燕云十六声",
                "screencap": "PrintWindow",
                "mouse": "SendMessage",
                "keyboard": "SendMessage"
            }
        }
    ],
    "resource": [
        {
            "name": "官服",
            "path": [
                "{PROJECT_DIR}/resource"
            ]
        }
    ],
    // 可选的 agent 示例
    // "agent": {
    //     "child_exec": "python",
    //     "child_args": [
    //         "{PROJECT_DIR}/agent/main.py"
    //     ]
    // },
    "task": [
        {
            "name": "太极炸鱼",
            "entry": "太极炸鱼"
        },
        {
            "name": "收集",
            "entry": "collection"
        }
    ],
    "option": {
        "收集": {
            "label": "有没有采集马",
            "type": "switch",
            "cases": [
                {
                    "name": "No"
                },
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "entry": "采集马"
                    }
                }
            ]
        }
    }
}